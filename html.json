	"BG Image w/ VML": {
		"prefix": "tbl5",
		"body": [
		  "<table width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" bgcolor=\"#${1:backgroundColor}\" role=\"presentation\">",
		  "    <tr>",
		  "        <td align=\"center\" valign=\"top\" height=\"${2:height}\" background=\"images/${3:imageSrc}\">",
