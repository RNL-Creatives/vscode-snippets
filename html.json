		  " ",
		  "        <!--[if gte mso 9]>",
		  "        <v:rect xmlns:v=\"urn:schemas-microsoft-com:vml\" fill=\"true\" stroke=\"false\" style=\"width:${4:width}pt;height:${5:height}pt;\">",
