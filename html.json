{	
	"Table only": {
		"prefix": "tbl",
		"body": [
		  "<table width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" role=\"presentation\" bgcolor=\"#${1:backgroundColor}\">",
		  "    <tr>    ",
		  "        $2",
		  "    </tr>",
		  "</table>"
		],
		"description": "Table only"
	},
	"Full-width fluid image": {
		"prefix": "tbl1",
		"body": [
		  "<table width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" bgcolor=\"#${1:background color}\" role=\"presentation\">",
		  "    <tr>",
		  "        <td width=\"100%\">",
		  "            <img src=\"${2:images}\" width=\"100%\" border=\"0\" alt=\"\" title=\"\" style=\"display:block;\" />",
		  "        </td>",
		  "    </tr>",
		  "</table>"
		],
		"description": "Full-width fluid image"
	},
	"Fluid image of any width floating in a full-width container": {
		"prefix": "tbl2",
		"body": [
		  "<table width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\" bgcolor=\"#${1:background color}\" role=\"presentation\">",
		  "    <tr>",
		  "        <td width=\"100%\" align=\"center\">",
		  "            <img class=\"imageMaxWidth\" src=\"${2:images}\" width=\"100%\" border=\"0\" alt=\"\" title=\"\" style=\"display:block; max-width: ${3: Edit width}px;\" />",
		  "        </td>",
		  "    </tr>",
		  "</table>"
